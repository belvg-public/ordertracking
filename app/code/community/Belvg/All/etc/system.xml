<config>
    <tabs>
        <belvg translate="label" module="belvgall">
            <label><![CDATA[<div style="background:url(data:image/png;base64,) no-repeat; width: 155px; height: 20px;"></div>]]>
            </label>
            <sort_order>300</sort_order>
        </belvg>
    </tabs>

    <sections>
        <belvgall translate="label">
            <label>General</label>
            <tab>belvg</tab>
            <frontend_type>text</frontend_type>
            <sort_order>777</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
                <extensions translate="label">
                    <label>Installed Extensions</label>
                    <frontend_type>text</frontend_type>
                    <frontend_model>belvgall/extensions</frontend_model>
                    <sort_order>2</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                </extensions>
                <feed>
                    <label>Notifications</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>90</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <interests translate="label">
                            <label>I'd like to be informed by Belvg about:</label>
                            <comment></comment>
                            <frontend_type>multiselect</frontend_type>
                            <sort_order>100</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <can_be_empty>1</can_be_empty>
                            <source_model>belvgall/source_updates_type</source_model>
                        </interests>
                    </fields>
                </feed>
            </groups>
        </belvgall>
        <related>
            <label>Related Extensions</label>
            <tab>belvg</tab>
            <frontend_type>text</frontend_type>
            <sort_order>9999</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
                <extensions translate="label">
                    <label>Related Extensions</label>
                    <frontend_type>text</frontend_type>
                    <frontend_model>belvgall/store</frontend_model>
                    <sort_order>2</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                </extensions>
            </groups>
        </related>
    </sections>
</config>